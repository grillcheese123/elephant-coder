an(r[\\\"latency_ms\\\"] for r in valid_runs)\\n    total_cost_usd = sum(r[\\\"cost_usd\\\"] for r in valid_runs)\\n\\n    return {\\n        \\\"runs_total\\\": runs_total,\\n        \\\"success_rate\\\": success_rate,\\n        \\\"avg_total_tokens\\\": round(avg_total_tokens, 2),\\n        \\\"avg_latency_ms\\\": round(avg_latency_ms, 2),\\n        \\\"total_cost_usd\\\": round(total_cost_usd, 6),\\n    }\"\n    }\n  ],\n  \"dependencies\": {\n    \"dependencies\": {},\n    \"devDependencies\": {}\n  },\n  \"commands\": []\n}\n```"
