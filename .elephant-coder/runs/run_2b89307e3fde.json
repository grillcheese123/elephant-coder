        "content": "{\"summary\": \"Refactor context-profile selection into cleaner strategy functions for maintainability.\", \"project\": {\"language\": \"python\"}, \"files\": [{\"path\": \"elephant_coder/context_profiles.py\", \"content\": \"\\\"\\\"\\\"Context profile selection strategies for Elephant Coder.\\\"\\\"\\\"\\n\\nfrom __future__ import annotations\\n\\nfrom dataclasses import dataclass\\nfrom pathlib import Path\\nfrom typing import Callable\\n\\n\\n@dataclass\\nclass ContextProfile:\\n    \\\"\\\"\\\"Represents a context selection strategy.\\\"\\\"\\\"\\n    name: str\\n    description: str\\n    selector: Callable[[list[str], Path], list[str]]\\n\\n\\ndef _select_by_git_diff(changed_files: list[str], project_root: Path) -> list[str]:\\n    \\\"\\\"\\\"Select files based on git diff (placeholder for future implementation).\\\"\\\"\\\"\\n    return changed_files\\n\\n\\ndef _select_by_affected_symbols(changed_files: list[str], project_root: Path) -> list[str]:\\n    \\\"\\\"\\\"Select files based on symbol dependencies (placeholder for future implementation).\\\"\\\"\\\"\\n    return changed_files\\n\\n\\ndef _select_by_import_tree(changed_files: list[str], project_root: Path) -> list[str]:\\n    \\\"\\\"\\\"Select files based on import tree traversal.\\\"\\\"\\\"\\n    return changed_files\\n\\n\\ndef _select_all(changed_files: list[str], project_root: Path) -> list[str]:\\n    \\\"\\\"\\\"Select all files for full context.\\\"\\\"\\\"\\n    return changed_files\\n\\n\\n# Available context profiles\\nPROFILES = {\\n    \\\"minimal\\\": ContextProfile(\\n        name=\\\"minimal\\\",\\n        description=\\\"Use only directly changed files.\\\",\\n        selector=_select_by_git_diff,\\n    ),\\n    \\\"symbols\\\": ContextProfile(\\n        name=\\\"symbols\\\",\\n        description=\\\"Include files with affected symbol definitions.\\\",\\n        selector=_select_by_affected_symbols,\\n    ),\\n    \\\"imports\\\": ContextProfile(\\n        name=\\\"imports\\\",\\n        description=\\\"Include files in the import dependency tree.\\\",\\n        selector=_select_by_import_tree,\\n    ),\\n    \\\"full\\\": ContextProfile(\\n        name=\\\"full\\\",\\n        description=\\\"Include all indexed files.\\\",\\n        selector=_select_all,\\n    ),\\n}\\n\\n\\ndef select_context(profile_name: str, changed_files: list[str], project_root: Path) -> list[str]:\\n    \\\"\\\"\\\"Select context files using the specified profile.\\\"\\\"\\\"\\n    profile = PROFILES.get(profile_name)\\n    if profile is None:\\n        raise ValueError(f\\\"Unknown context profile: {profile_name}\\\")\\n    return profile.selector(changed_files, project_root)\\n\"}], \"dependencies\": {\"dependencies\": {}, \"devDependencies\": {}, \"python\": [], \"pythonDev\": [], \"rust\": [], \"go\": [], \"cpp\": []}, \"commands\": [\"python -c \\\"from elephant_coder.context_profiles import select_context, PROFILES; print(list(PROFILES.keys()))\\\"\"]}"
